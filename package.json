{
  "name": "@monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "app": "sh -c 'pnpm exec nx g @nx/node:application ${0} --directory=apps/'",
    "lib": "sh -c 'pnpm exec nx g @nx/js:lib ${0} --directory=libs/'",
    "dev": "sh -c 'pnpm exec nx serve ${0}'",
    "show-web": "sh -c 'pnpm exec nx show project ${0} --web'",
    "test:all": "pnpm exec nx run-many --target=test --all",
    "test": "sh -c 'pnpm exec npx nx test ${0} --verbose'",
    "lint:all": "pnpm exec nx run-many --target=lint --all",
    "lint:all:fix": "pnpm exec nx run-many --target=lint --all --fix",
    "lint": "sh -c 'pnpm exec npx nx lint ${0} --verbose'",
    "lint:fix": "sh -c 'pnpm exec nx lint ${0} --fix --verbose'",
    "build:all": "pnpm exec nx run-many --target=build --all",
    "build": "sh -c 'pnpm exec nx build ${0} --verbose'",
    "dc:s": "sh -c 'docker compose up ${0}'",
    "dc": "sh -c 'docker compose up --build'"
  },
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@nx/esbuild": "19.1.1",
    "@nx/eslint": "19.1.1",
    "@nx/eslint-plugin": "19.1.1",
    "@nx/express": "19.1.1",
    "@nx/jest": "19.1.1",
    "@nx/js": "19.1.1",
    "@nx/node": "19.1.1",
    "@nx/web": "19.1.1",
    "@nx/webpack": "19.1.1",
    "@nx/workspace": "19.1.1",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/body-parser": "^1.19.5",
    "@types/express": "~4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.16.9",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "babel-jest": "^29.4.1",
    "esbuild": "^0.19.2",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "lint-staged": "^15.2.5",
    "nx": "19.1.1",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.4.2",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "20.12.2",
    "pnpm": "8.15.5"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm run lint:all:fix"
    ]
  }
}
