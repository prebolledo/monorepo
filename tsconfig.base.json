{
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "declaration": true,
    "target": "ES2022",
    "module": "CommonJS",
    "sourceMap": true,
    "resolveJsonModule": true,
    "noEmit": false
  }
}